<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<UserSecretsId>f3e3439f-a03e-43f0-ba7f-8bdcc253f3cf</UserSecretsId>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Datasync" Version="5.1.5" />
		<PackageReference Include="Microsoft.AspNetCore.Datasync.EFCore" Version="5.1.5" />
		<PackageReference Include="Microsoft.AspNetCore.Datasync.NSwag" Version="5.1.5" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Cosmos" Version="7.0.5" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="7.0.5">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="NSwag.AspNetCore" Version="13.19.0" />
	</ItemGroup>
</Project>
